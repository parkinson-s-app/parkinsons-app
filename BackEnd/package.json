{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "build/bin/www.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && export DEBUG=AppKinson:* && node ./build/bin/www.js",
    "dev": "nodemon --watch src -e ts,js --exec npm start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "docker": "docker build -t back .",
    "docker:d": "docker run -d -p 8000:8000 back",
    "docker:it": "docker run -it -p 8000:8000 back",
    "docker-up": "docker-compose up",
    "docker-build": "docker-compose build",
    "docker-down": "docker-compose down -v && npm run restartdb",
    "restartdb": "sudo rm -r src/db/data",
    "deleteOut": "test -f output.txt && rm output.txt || echo \"doesnt exists\"",
    "rund": "export DEBUG=AppKinson:* && nohup node ./build/bin/www.js >> output.txt 2>&1 &",
    "rundocker": "export DEBUG=AppKinson:* && node ./build/bin/www.js >> ./output.txt 2>&1 "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.10",
    "@types/uuid": "^8.3.0",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "express-actuator": "^1.7.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "uuid": "^8.3.2"
  }
}
